{
	"name": "SQR",
	"displayName": "SQR",
	"description": "SQR Language Support (Oracle, PeopleSoft Enterprise)",
	"version": "0.1.0",
	"publisher": "CityOfEscondido",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [{
			"id": "sqr",
			"aliases": ["SQR", "sqr"],
